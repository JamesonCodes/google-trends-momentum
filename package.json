{
  "name": "rising-topics",
  "version": "0.1.0",
  "description": "A modern web application for discovering trending and rising topics across various categories",
  "keywords": ["trending", "topics", "google-trends", "nextjs", "typescript", "data-visualization", "ai", "business", "tech"],
  "author": "Jameson Campbell",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesonCodes/google-trends-momentum.git"
  },
  "homepage": "https://github.com/JamesonCodes/google-trends-momentum#readme",
  "bugs": {
    "url": "https://github.com/JamesonCodes/google-trends-momentum/issues"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:python": "python tests/run_tests.py",
    "test:mvp": "python tests/test_mvp_functionality.py",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security": "npm audit --audit-level moderate",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.4"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "@eslint/eslintrc": "^3"
  }
